<div class="col-xs-12">
  <h1 class="page-header">Data load requested</h1>
  <h4><%= @data_file %> read in and processed.</h4>

  <div class="row">
    <div class="col-xs-6 col-sm-2 col-md-1 text-right"><%= @num_rows_processed %></div>
    <div class="col-xs-6 col-sm-10 col-md-11">Reports processed</div>
  </div>

  <div class="row">
    <div class="col-xs-6 col-sm-2 col-md-1 text-right"><%= @num_reports %></div>
    <div class="col-xs-6 col-sm-10 col-md-11">Total reports</div>
  </div>

  <div class="row">
    <div class="col-xs-6 col-sm-2 col-md-1 text-right"><%= @num_activities %></div>
    <div class="col-xs-6 col-sm-10 col-md-11">Activities</div>
  </div>

  <div class="row">
    <div class="col-xs-6 col-sm-2 col-md-1 text-right"><%= @num_locations %></div>
    <div class="col-xs-6 col-sm-10 col-md-11">Locations</div>
  </div>

  <div class="row">
    <div class="col-xs-6 col-sm-2 col-md-1 text-right"><%= @num_people %></div>
    <div class="col-xs-6 col-sm-10 col-md-11">People</div>
  </div>

  <div class="row">
    <div class="col-xs-6 col-sm-2 col-md-1 text-right"><%= @num_feelings %></div>
    <div class="col-xs-6 col-sm-10 col-md-11">Feelings</div>
  </div>

  <div class="row">
    <div class="col-xs-6 col-sm-2 col-md-1 text-right"><%= @num_feeling_reasons %></div>
    <div class="col-xs-6 col-sm-10 col-md-11">Feeling reasons</div>
  </div>

  <div class="row">
    <div class="col-xs-6 col-sm-2 col-md-1 text-right"><%= @num_weathers %></div>
    <div class="col-xs-6 col-sm-10 col-md-11">Weather types</div>
  </div>

  <div class="row">
    <div class="col-xs-6 col-sm-2 col-md-1 text-right"><%= @num_noise_level_descriptions %></div>
    <div class="col-xs-6 col-sm-10 col-md-11">Noise level descriptions</div>
  </div>
</div>
